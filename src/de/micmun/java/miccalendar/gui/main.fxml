                <Label alignment="CENTER" contentDisplay="CENTER" text="Monat: ">
                    <HBox.margin>
                        <Insets left="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
                <ComboBox fx:id="cbMonat">
                    <HBox.margin>
                        <Insets left="10.0" top="10.0" />
                    </HBox.margin>
                </ComboBox>
                <Label alignment="CENTER" contentDisplay="CENTER" text="Jahr: ">
                    <HBox.margin>
                        <Insets left="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
                <ComboBox fx:id="cbJahr">
                    <HBox.margin>
                        <Insets left="10.0" top="10.0" />
                    </HBox.margin>
                </ComboBox>
            </children>
        </HBox>
    </bottom>
    <top>
        <HBox fx:id="hbTop" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
